{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "RoleWithEmptyName": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version" : "2012-10-17",
          "Statement": [ {
            "Effect": "Allow",
            "Principal": {
              "Service": [ "ec2.amazonaws.com" ],
              "AWS" : "arn:aws:iam::324320755747:root"
            },
            "Action": ["sts:AssumeRole"]
          } ]
        },
        "Path": "/",
        "RoleName": "",
        "Policies": [ {
          "PolicyName": "root",
          "PolicyDocument": {
            "Version" : "2012-10-17",
            "Statement": [ {
              "Effect": "Allow",
              "Action": "*",
              "Resource": "*"
            } ]
          }
        } ]
      }
    }
  }
}
