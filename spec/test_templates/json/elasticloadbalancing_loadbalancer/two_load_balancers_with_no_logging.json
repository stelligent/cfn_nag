          "Statement": [
            {
              "Sid": "Stmt1429136633762",
              "Action": [
                "s3:PutObject"
              ],
              "Effect": "Allow",
              "Resource": "arn:aws:s3:::fakebucketfakebucket/prefix/AWSLogs/111111111111/*",
              "Principal": {
                "AWS": [
                  "156460612806"
                ]
              }
            }
          ]
        }
      }
    }
  }
