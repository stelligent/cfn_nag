        },
        "AvailabilityZones" : [
          "eu-west-1a"
        ],
        "Listeners" : [
          {
            "LoadBalancerPort" : "80",
            "InstancePort" : "80",
            "Protocol" : "HTTP"
          }
        ]
      }
    },
    "elb3": {
      "Type" : "AWS::ElasticLoadBalancing::LoadBalancer",
      "DependsOn": "S3BucketPolicy",
      "Properties" : {
        "AccessLoggingPolicy": {
          "Enabled": true,
          "S3BucketName": "fakebucketfakebucket",
          "S3BucketPrefix": "prefix"
